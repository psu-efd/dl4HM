{
  "case": {
    "name": "test"
  },
  "dataLoader": {
	"training_data": "./SRH-2D/train_uvonly.tfrecords",
	"validation_data": "./SRH-2D/validation_uvonly.tfrecords",
	"test_data": "./SRH-2D/test_uvonly.tfrecords",
	"scaled_data": true,
	"minMaxVars_file": "varialbes_min_max.json"
  },
  "model":{
	"comment": "model_type choices: fully_connected_MLP, CNN",
	"model_type": "CNN",
    "learning_rate": 0.0001,
    "optimizer": "adam"
  },
  "trainer":{
    "num_epochs": 100,
    "batch_size": 20,
    "verbose_training": 1
  },
  "inverter":{
	"do_inversion": true,
	"optimizer": "adam",
	"learning_rate": 0.1,
	"inversion_data": "backwater_curve_inversion_data.npz"
  },
  "callbacks":{
    "checkpoint_monitor": "val_loss",
    "checkpoint_mode": "min",
    "checkpoint_save_best_only": true,
    "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true
  }
}
